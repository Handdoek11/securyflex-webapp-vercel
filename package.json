  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check . && tsc --noEmit",
    "check:fix": "biome check --write . && tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run --config src/test/integration/vitest.integration.config.ts src/test/integration",
    "test:integration:watch": "vitest --config src/test/integration/vitest.integration.config.ts src/test/integration",
    "test:integration:coverage": "vitest run --coverage --config src/test/integration/vitest.integration.config.ts src/test/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "backup": "ts-node scripts/backup/database-backup.ts",
    "backup:schedule": "ts-node scripts/backup/backup-scheduler.ts",
    "backup:start": "ts-node scripts/backup/backup-scheduler.ts start",
    "backup:stop": "ts-node scripts/backup/backup-scheduler.ts stop",
    "backup:status": "ts-node scripts/backup/backup-scheduler.ts status",
    "backup:health": "ts-node scripts/backup/backup-scheduler.ts health",
    "backup:restore": "ts-node scripts/backup/backup-restore.ts",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "db:seed": "tsx prisma/seed.ts",
    "db:migrate": "tsx prisma/migrations/data-migration.ts",
    "e2e:debug": "playwright test --debug",
    "e2e:report": "playwright show-report",
    "e2e:install": "playwright install",
    "admin:unlock-user": "node scripts/admin/unlock-user.js",
    "admin:reset-password": "node scripts/admin/reset-user-password.js",
    "admin:daily-report": "node scripts/admin/daily-report.js",
    "admin:export-users": "node scripts/admin/export-users.js",
    "admin:cleanup-test": "node scripts/admin/cleanup-test-data.js",
    "admin:check": "node check-admin-accounts.js",
    "admin:create": "node create-admin-users.js"
